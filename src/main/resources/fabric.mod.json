{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contributors": [
    "${mod_credits}"
  ],
  "contact": {
    "homepage": "${mod_homepage}",
    "sources": "${mod_source}",
    "issues": "${mod_issues}"
  },
  "license": "${mod_license}",
  "icon": "assets/bedrockskins/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.brandonitaly.bedrockskins.BedrockSkins"
    ],
    "client": [
      "com.brandonitaly.bedrockskins.client.BedrockSkinsClient"
    ],
    "modmenu": [
      "com.brandonitaly.bedrockskins.client.ModMenuIntegration"
    ]
  },
  "accessWidener": "${aw}",
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabric-api": "*",
    "fabricloader": ">=0.17",
    "minecraft": ["${mc_version_range}"]
  },
  "suggests": {
    "legacy4j": "*"
  }
}
